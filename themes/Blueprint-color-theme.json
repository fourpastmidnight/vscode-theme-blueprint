{
  "name": "Blueprint",
  "type": "dark",
  "colors": {
    "selection.background": "#2B95D6",
    "errorForeground": "#DB3737",
    "badge.background": "#137CBD",
    "button.background": "#137CBD",
    "button.hoverBackground": "#106BA3",
    "button.foreground": "#FFFFFF",
    "input.background": "#10161A4d",
    "input.border": "#10161A",
    "input.placeholderForeground": "#bfccd680",
    "inputOption.activeBorder": "#106BA3",
    "inputValidation.errorBorder": "#DB3737",
    "inputValidation.infoBorder": "#137CBD",
    "inputValidation.warningBorder": "#D9822B",
    "activityBar.background": "#30404D",
    "activityBar.border": "#10161A",
    "sideBar.background": "#182026",
    "sideBar.border": "#10161A",
    "sideBarSectionHeader.background": "#202B33",
    "editorGroupHeader.tabsBackground": "#182026",
    "tab.inactiveBackground": "#202B33",
    "tab.activeBackground": "#293742",
    "tab.activeBorder": "#137CBD",
    "tab.border": "#10161A",
    "editorGroupHeader.tabsBorder": "#10161A",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.activeSelectionBackground": "#0E5A8A",
    "list.hoverBackground": "#202B33",
    "list.inactiveSelectionBackground": "#0E5A8A4d"
  },
  "settings": [
    {
      "settings": {
        "background": "#182026",
        "caret": "#F5F8FA",
        "foreground": "#BFCCD6",
        "invisibles": "#394B59",
        "lineHighlight": "#293742",
        "selection": "#106BA3",
        "findHighlight": "#FFE792",
        "findHighlightForeground": "#10161A",
        "selectionBorder": "#222218",
        "activeGuide": "#9D550FB0",
        "bracketsForeground": "#48AFF0",
        "bracketsOptions": "underline",
        "bracketContentsForeground": "#48AFF0",
        "bracketContentsOptions": "underline",
        "tagsOptions": "stippled_underline"
      }
    },
    {
      "scope": [
        "punctuation.definition",
        "punctuation.section",
        "meta.brace",
        "tag.assignment",
        "markup.heading.name"
      ],
      "settings": {
        "foreground": "#F5F8FA"
      }
    },
    {
      "scope": [
        "comment",
        "comment.line",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#738694"
      }
    },
    {
      "scope": [
        "string",
        "string.scss",
        "punctuation.definition.string",
        "markup.punctuation.quote"
      ],
      "settings": {
        "foreground": "#B6D94C"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "beginning.punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#F5498B"
      }
    },
    {
      "scope": [
        "variable.object.property",
        "variable.other.property",
        "variable.other.constant.property",
        "variable.other.object.property",
        "support.variable.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#BFCCD6"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.other.attribute-name.class.mixin",
        "meta.decorator",
        "meta.decorator variable",
        "meta.link.inline",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#2B95D6"
      }
    },
    {
      "scope": [
        "variable",
        "markup.inline.raw",
        "punctuation.definition.variable.less"
      ],
      "settings": {
        "foreground": "#00B3A4"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.legacy.builtin",
        "support.class",
        "support.module",
        "support.type",
        "support.variable",
        "constant.character",
        "constant.other",
        "support.constant"
      ],
      "settings": {
        "foreground": "#2EE6D6"
      }
    },
    {
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#AD99FF"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "entity.name.tag support.class.component",
        "support.type.property-name.json",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#29A634"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage",
        "storage.type",
        "storage.type punctuation",
        "storage.modifier",
        "markup.bold",
        "punctuation.definition.bold",
        "punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#A854A8"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "entity.name.tag.reference",
        "entity.other.attribute-name.parent-selector punctuation.definition",
        "markup.italic",
        "punctuation.definition.italic",
        "punctuation.definition.interpolation",
        "punctuation.definition.template-expression",
        "punctuation.definition.typeparameters",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#C274C2"
      }
    },
    {
      "scope": [
        "entity",
        "punctuation.regex.group",
        "markup.heading.punctuation"
      ],
      "settings": {
        "foreground": "#F29D49"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.type.primitive",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#F2B824"
      }
    },
    {
      "scope": [
        "meta.type",
        "meta.return.type",
        "entity.name.type.interface",
        "meta.interface entity.other.inherited-class"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.type keyword.control",
        "meta.type storage.type.type"
      ],
      "settings": {
        "fontStyle": "normal"
      }
    }
  ]
}